{
    "testbed": "itcoin-testbed",
    "key": {
        "name": "itcoinKeyPair",
        "path": "~/.ssh/aws"
    },
    "ports": {
        "consensus": 13000,
        "bitcoind": 38000
    },
    "repo": {
        "name": "itcoin-fbft",
