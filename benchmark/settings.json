{
    "testbed": "itcoin-testbed",
    "key": {
        "name": "itcoinKeyPair",
        "path": "~/.ssh/aws"
    },
    "ports": {
        "consensus": 13000,
        "bitcoind": 38000
    },
    "repo": {
        "name": "itcoin-pbft",
        "url": "https://github.com/bancaditalia/itcoin-pbft.git",
        "branch": "benchmarking-framework", 
        "cmake_options": "-DCMAKE_C_COMPILER=gcc-10 -DCMAKE_CXX_COMPILER=g++-10 -DITCOIN_CORE_GIT_URL=https://github.com/bancaditalia/itcoin-core.git -DITCOIN_CORE_GIT_REV=icbc23 -DITCOIN_SECP256K1_GIT_URL=https://github.com/bancaditalia/itcoin-secp256k1.git -DCMAKE_BUILD_TYPE=Debug"
    },
    "instances": {
        "type": "t3.xlarge",
        "regions": [
            "eu-west-1", "eu-central-1", "eu-south-1", "eu-north-1", "eu-west-2", "eu-central-2", "eu-south-2", "eu-west-3"
        ]
    }
}